#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
        keymap {
                compatible = "zmk,keymap";

                default_layer {
                        bindings = <
//,-----------------------------------------------------.                    ,-----------------------------------------------------.
    &kp ESC  &kp Q    &kp W    &kp E    &kp R    &kp T                          &kp Y   &kp U    &kp I    &kp O    &kp P   &kp LGUI
//|--------+--------+--------+--------+--------+--------|                    |--------+--------+--------+--------+--------+--------|
    &kp TAB  &kp A    &kp S    &kp D    &kp F    &kp G                          &kp H   &kp J    &kp K    &kp L   &kp SEMI &kp SQT
//|--------+--------+--------+--------+--------+--------|                    |--------+--------+--------+--------+--------+--------|
   &kp LSHFT &kp Z    &kp X    &kp C    &kp V    &kp B                          &kp N   &kp M  &kp COMMA &kp DOT &kp SLASH &kp LALT
//|-----------------------------------+--------+--------+--------|  |--------+--------+--------+-----------------------------------|
                                      &kp LCTRL &kp SPACE &mo 1        &mo 2   &kp RET  &kp BSPC
                        >;
                };
                layer_1 {
                        bindings = <
//,-----------------------------------------------------.                    ,-----------------------------------------------------.
   INT_KANA   &kp N1  &kp N2    &kp N3  &kp N4   &kp N5                        &kp N6   &kp N7   &kp N8   &kp N9   &kp N0   &trans
//|--------+--------+--------+--------+--------+--------|                    |--------+--------+--------+--------+--------+--------|
   &trans    &none    &none    &none   &kp LPAR &kp RPAR                      &kp LEFT &kp DOWN &kp UP   &kp RIGHT  &none    &none
//|--------+--------+--------+--------+--------+--------|                    |--------+--------+--------+--------+--------+--------|   
   &trans    &none    &none     &none  &kp LBKT &kp RBKT                       &none    &none    &none    &none    &none    &trans
//|-----------------------------------+--------+--------+--------|  |--------+--------+--------+-----------------------------------|
                                        &trans   &trans   &trans      &mo 3   &trans   &trans
                        >;
                };

                layer_2 {
                        bindings = <
//,-----------------------------------------------------.                    ,-----------------------------------------------------.
   INT_RO   &kp EXCL  &kp AT  &kp HASH &kp DLLR &kp PRCNT                    &kp CARET &kp AMPS &kp ASTRK &kp LPAR &kp RPAR &trans
//|--------+--------+--------+--------+--------+--------|                    |--------+--------+--------+--------+--------+--------| 
   &trans    &none    &none    &none   &kp LT  &kp GT                       &kp MINUS &kp EQUAL &kp SLASH &kp GRAVE &none &none
//|--------+--------+--------+--------+--------+--------|                    |--------+--------+--------+--------+--------+--------| 
   &trans    &none    &none    &none   &kp LBKT  &kp RBKT                   &kp UNDER &kp PLUS  &kp PIPE  &kp TILDE &none &trans
//|-----------------------------------+--------+--------+--------|  |--------+--------+--------+-----------------------------------|
                                        &trans   &trans   &mo 3      &trans   &trans   &trans
                        >;
                };


                layer_3 {
                        bindings = <
//,-----------------------------------------------------.                    ,-----------------------------------------------------.
    &none    &kp F1   &kp F2   &kp F3   &kp F4  &kp F5                          &kp F6  &kp F7   &kp F8   &kp F9  &kp F10   &trans
//|--------+--------+--------+--------+--------+--------|                    |--------+--------+--------+--------+--------+--------| 
    &trans   &trans   &none    &none    &none    &none                          &none   &none    &none    &none   &kp F11   &kp F12
//|--------+--------+--------+--------+--------+--------|                    |--------+--------+--------+--------+--------+--------| 
    &trans   &none    &none    &none    &none    &none                          &none   &none    &none    &none    &none   &trans
//|-----------------------------------+--------+--------+--------|  |--------+--------+--------+-----------------------------------|
                                        &trans   &trans   &trans      &trans   &trans   &trans
                        >;
                };
        };
};
